						startScrolling();
						function startScrolling()
						{
							timer=setInterval(function(){
								iNow++;
								if(iNow>liLength-1)
								{
									iNow=0;
								}
								oNumber.find("i").eq(iNow).addClass("active").siblings().removeClass();
								aBigImgLi.eq(iNow).fadeIn(iSpeed).siblings().fadeOut(iSpeed);
							},4000);
						}
						function stopScrolling()
						{
							clearInterval(timer);
						}
						$("#indexFoucs").hover(function(){
							stopScrolling();
						},function(){
							startScrolling();
						});
					}
					
					focusPic();
