//排序
function OBJ_SORT(tag){
	this.tag = tag; //1降序  0升序
	this.key_arr = new Array(new Array());
}
OBJ_SORT.prototype.doSort=function (key_arr){
		//alert(key_arr);

		if(this.tag == 0){
			key_arr.sort(function(a,b){return a[0]>b[0]?1:-1});	
		}else{
			key_arr.sort(function(a,b){return a[0]<b[0]?1:-1});
		}
		
		this.key_arr = key_arr;
	//	${JQUERY}each(key_arr, function(i,n){alert(n)});
	}
	OBJ_SORT.prototype.get =function(){
		return this.key_arr;
	}
	
function sortById(id,tag,key){
		obj = document.getElementById(id);
		child_nodes = obj.childNodes;
		var key_arr = new Array();
		${JQUERY}each(child_nodes, function(i,n){
			if($(this).get(0).tagName != undefined){
				key_arr.push(new Array(parseFloat($(this).attr(key)),$(this).attr('id')));
			}
		});	
		s = new OBJ_SORT(tag);
		s.doSort(key_arr);
		key_arr = s.get();
		o_arr = new Array();
		${JQUERY}each(key_arr, function(i,n){
			that = $('#'+n[1]);
			o = document.createElement('div');
			o.id = that.attr('id');
			o.setAttribute('price',that.attr('price'));
			o.setAttribute('comtot',that.attr('comtot'));
			o.className = that.attr('class');
			o.innerHTML = that.html();
			o_arr.push(o);
		});
		obj.innerHTML = '';
		${JQUERY}each(o_arr,function(i,n){
			obj.appendChild(n);
		});
	};

function sortComment(id,tag,key){
		obj = document.getElementById(id);
		child_nodes = obj.childNodes;
		var key_arr = new Array();
		box2_arr = new Array();
		${JQUERY}each(child_nodes, function(i,n){
			if($(this).get(0).tagName != undefined && $(this).attr('class') == 'box'){
				key_arr.push(new Array(parseFloat($(this).attr(key)),$(this).attr('id')));
			}
			if($(this).attr('class') == 'box2'){
				box2_arr.push(n);
			}
		});	
		s = new OBJ_SORT(tag);
		s.doSort(key_arr);
		key_arr = s.get();
		o_arr = new Array();
		${JQUERY}each(key_arr, function(i,n){
			that = $('#'+n[1]);
			o = document.createElement('div');
			o.id = that.attr('id');
			o.setAttribute('price',that.attr('price'));
			o.setAttribute('comtot',that.attr('comtot'));
			o.className = that.attr('class');
			o.innerHTML = that.html();
			o_arr.push(o);
		});
		obj.innerHTML = '';
		${JQUERY}each(o_arr,function(i,n){
			obj.appendChild(n);
		});
		${JQUERY}each(box2_arr, function(i,n){obj.appendChild(n)});
		
}

