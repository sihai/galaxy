							if(i==0)
							{
								oNumber.append($("<i class='active'>"+(i+1)+"</i>"));
							}
							else
							{
								oNumber.append($("<i>"+(i+1)+"</i>"));
							}
						}
						oNumber.find("i").mouseover(function(){
							iNow=oNumber.find("i").index($(this));
							oNumber.find("i").eq(iNow).addClass("active").siblings().removeClass();
							aBigImgLi.eq(iNow).fadeIn(iSpeed).siblings().hide();
							var thisImg=aBigImgLi.eq(iNow).find('img');
							thisImg.attr('src',thisImg.attr('_src'));
