				if(price1v==''){
					price1v=0;
				}
				if(price2v==''||price2v=='不限'){
					price2v='';
				}
				if(price2v==''){
					price2v=99999999;
				}
		
				if (parseInt(price1v) > parseInt(price2v)) {
					alert('最低价格不能大于最高价格');
					return false;
				}
				url = window.location.href;
				url = url.replace(/[?|&]price=[^&]*/i, '');
				url = url.replace(/[?|&]menu=[^&]*/i, '');
				url = url + "&menu=1&price=" + encodeURIComponent(price1v + '-' + price2v);
				window.location = url;
			});
