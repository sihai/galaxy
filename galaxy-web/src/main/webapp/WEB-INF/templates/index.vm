					function focusPic()
					{
						var oBigImg=$("#bigImg");
						var aBigImgLi=$("#bigImg li");
						var liLength=aBigImgLi.length;
						var oNumber=$("#number");
						var iZindex=liLength;
						var iNow=0;
						var timer=null;
						var iSpeed=500;
						for(var i=0; i<liLength; i++)
