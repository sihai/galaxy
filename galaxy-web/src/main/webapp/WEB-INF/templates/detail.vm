	var patt = new RegExp('utm_campaign');
	if(patt.test(d_url))
	{
		var pattern = /utm_campaign=[^&]*/;
		strs = d_url.match(pattern);
		var patt2 = new RegExp('javascript');
		var patt3 = new RegExp('utm_campaign');
		var patt4 = new RegExp('#tabbar');
		$('a').each(function(){
			if(!patt2.test(this.href) && !patt3.test(this.href)){
				var lastw = this.href.substr(-1);
				var last3 = this.href.substr(-3);
				var last5 = this.href.substr(-5);
				if(lastw=='/')
				{
					this.href = this.href+'?'+strs;
				}else if(lastw==''){
					this.href ='';
				}else if(last3=='jpg' || last5='shtml'){
					this.href =this.href;
				}else{
					this.href = this.href+'&'+strs;
				}
				if(patt4.test(this.href))
				{
					str2 = this.href.match(patt4);
					this.href = this.href.replace(/#tabbar/g,'');
					this.href = this.href+str2;
				}
			}
		});
	}
